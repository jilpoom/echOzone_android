<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".info_cap">

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:fontFamily="@font/pretendardvariable"
        android:text="내 가게 수거함 현황"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button3"
        android:layout_width="93dp"
        android:layout_height="30dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/store_info_buttonround"
        android:fontFamily="@font/pretendardvariable"
        android:text="즉시 알림전송"
        android:textColor="#FFFFFF"
        android:textSize="11sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.066"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button" />

    <Button
        android:id="@+id/button"
        android:layout_width="93dp"
        android:layout_height="30dp"
        android:layout_marginTop="324dp"
        android:background="@drawable/history_button"
        android:fontFamily="@font/pretendardvariable"
        android:text="업체 방문예약"
        android:textColor="@color/blue"
        android:textSize="11sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/button3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/button3"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="370dp"
        android:layout_height="108dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="416dp"
        android:background="@drawable/history_round1"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView21"
            android:layout_width="70dp"
            android:layout_height="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/pretendardvariable"
            android:text="현재 적재율"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tv_text"
            android:layout_width="33dp"
            android:layout_height="33dp"
            android:layout_marginLeft="315dp"
            android:layout_marginTop="-30dp"
            android:fontFamily="@font/pretendardvariable"
            android:text="84"
            android:textColor="@color/blue"
            android:textSize="26sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView23"
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="3dp"
            android:fontFamily="@font/pretendardvariable"
            android:text="적재율 100%예상 날짜"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/textView24"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="3dp"
            android:fontFamily="@font/pretendardvariable"
            android:text="하루 평균 적재량"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/textView26"
            android:layout_width="105dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="250dp"
            android:layout_marginTop="-42dp"
            android:fontFamily="@font/pretendardvariable"
            android:text="2022년 5월 25일"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/textView27"
            android:layout_width="30dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="323dp"
            android:fontFamily="@font/pretendardvariable"
            android:text="50개"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/textView28"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_marginLeft="350dp"
            android:layout_marginTop="-62dp"
            android:text="%"
            android:textColor="#000000"
            android:textStyle="bold" />

    </LinearLayout>

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.454"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.971"
        app:srcCompat="@drawable/backgroundbar" />

    <Button
        android:id="@+id/sc_store1"
        android:layout_width="30dp"
        android:layout_height="32dp"
        android:layout_marginStart="256dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/storeicon"
        app:layout_constraintBottom_toBottomOf="@+id/imageView19"
        app:layout_constraintStart_toStartOf="@+id/imageView19"
        app:layout_constraintTop_toTopOf="@+id/imageView19"
        app:layout_constraintVertical_bias="0.693" />

    <Button
        android:id="@+id/sc_calendar1"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="52dp"
        android:background="@drawable/calendaricon"
        app:layout_constraintStart_toStartOf="@+id/imageView19"
        app:layout_constraintTop_toTopOf="@+id/imageView19" />

    <Button
        android:id="@+id/sc_home1"
        android:layout_width="28dp"
        android:layout_height="25dp"
        android:layout_marginStart="185dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/homeicon"
        app:layout_constraintStart_toStartOf="@+id/imageView19"
        app:layout_constraintTop_toTopOf="@+id/imageView19" />

    <Button
        android:id="@+id/sc_menu1"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="324dp"
        android:layout_marginTop="55dp"
        android:background="@drawable/menuicon"
        app:layout_constraintStart_toStartOf="@+id/imageView19"
        app:layout_constraintTop_toTopOf="@+id/imageView19" />

    <Button
        android:id="@+id/sc_machine1"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="54dp"
        android:background="@drawable/plusicon"
        app:layout_constraintStart_toStartOf="@+id/imageView19"
        app:layout_constraintTop_toTopOf="@+id/imageView19" />

    <ImageView
        android:id="@+id/info80"
        android:layout_width="302dp"
        android:layout_height="331dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="48dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/a80" />

    <ImageView
        android:id="@+id/info100"
        android:layout_width="302dp"
        android:layout_height="331dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="48dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/a100" />

</androidx.constraintlayout.widget.ConstraintLayout>